@{
    Layout = null;
}
<style>
    div.ng-enter {
        -webkit-transition: 0.5s linear all; /* Chrome */
        transition: 1s linear all;
        opacity: 0.5;
    }

    div.ng-enter-active {
        opacity: 1;
    }

    .realtime-box {
        height: 100px;
        padding: 20px;
    }

    .charts .chart {
        height: 500px;
        margin-top: 10px
    }
</style>
<p>
    <strong>Realtime</strong>
</p>
<div style="" class="realtime-box bg-info">
    <div ng-repeat="t in traces">
        Time:{{t.Time|date:'yyyy-MM-dd HH:mm'}}&nbsp;&nbsp;
        Topic:{{t.Topic}}&nbsp;&nbsp;
        Level:{{t.Level}}&nbsp;&nbsp;
        Message:{{t.Message}}&nbsp;&nbsp;
    </div>
</div>
<div class="text-info">
    最近30天
</div>
<div class="charts row">
    <div class="chart col-md-4" echart chart-option="item" ng-repeat="item in chartsOptions"></div>
</div>